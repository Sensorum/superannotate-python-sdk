[BASIC]
good-names=df,f,e,i,j,k,im,pt,pr,m

bad-functions=
  apply,
  exit,
  input,
  quit

[TYPECHECK]
generated-members=cv2.*

[ELIF]
max-nested-blocks=5

[MASTER]
load-plugins=pylint_pytest
persistent=yes
ignore=pycocotools_sa

[MESSAGE_CONTROL]
disable=
    no-else-return,
    too-many-lines,
    too-many-arguments,
    too-many-instance-attributes

[SPELLING]

max-spelling-suggestions=5
spelling-dict=
spelling-ignore-words=bool,str,strs
spelling-private-dict-file=
spelling-store-unknown-words=no
